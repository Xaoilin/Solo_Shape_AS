<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
	
	<inherits name='Efficiency' />
	<inherits name='GameObjects' />
	<inherits name='GameWorld' />
	<inherits name='Levels' />
	<inherits name='Screens' />
	<inherits name='SSGame' />
	<inherits name='SSHelpers' />
	<inherits name='TweenAccessors' />
	<inherits name='ui' />
	
	<inherits name='aurelienribon.tweenengine' />
	<inherits name='com.badlogic.gdx.pay' />
	
	<entry-point class='com.xaoilin.soloshape.client.HtmlLauncher' />

	<set-configuration-property name="gdx.assetpath"
		value="../android/assets" />
	<extend-configuration-property name="gdx.reflect.include" value="SSGame" />
	<extend-configuration-property name="gdx.reflect.include" value="com.xaoilin.Efficiency" />
	<extend-configuration-property name="gdx.reflect.include" value="Screens" />
	<extend-configuration-property name="gdx.reflect.include" value="SSHelpers" />
	<extend-configuration-property name="gdx.reflect.include" value="TweenAccessors" />

	
  <extend-property name="user.agent" values="gecko1_8" />
  <extend-property name="user.agent" values="safari" />
  <extend-property name="user.agent" values="ie9" fallback-value="ie8" />
  <extend-property name="user.agent" values="ie10" />
</module>
